<Application x:Class="UDPRecorder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:l="clr-namespace:UDPRecorder"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary Source="Commands/AppCommands.xaml"/>
                 <ResourceDictionary Source="Styles/ControlsDefaults.xaml"/>
             </ResourceDictionary.MergedDictionaries>

             <l:ServiceLocator x:Key="Locator"/>

         </ResourceDictionary>
    </Application.Resources>
</Application>
